VITE_API_BASE='http://localhost:3001/api' # Use this URL when developing locally
# VITE_API_BASE="https://$CODESPACE_NAME-3001.$GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN/api" # for GitHub Codespaces
# VITE_API_BASE='/api' # Use this URL deploying on non-localhost address OR in docker.

# Keycloak Frontend Configuration (Optional - for production SSO)
# Enable Keycloak authentication in the frontend
VITE_KEYCLOAK_ENABLED=false

# Keycloak server URL (without /auth suffix for Keycloak 17+)
# VITE_KEYCLOAK_URL=http://localhost:8180

# Keycloak realm name
# VITE_KEYCLOAK_REALM=anythingllm

# Keycloak public client ID for frontend
# VITE_KEYCLOAK_CLIENT_ID=anythingllm-frontend
